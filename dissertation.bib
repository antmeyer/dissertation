@article{hammarstrom:2011,
Abstract = {This article surveys work on Unsupervised Learning of Morphology. We define Unsupervised Learning of Morphology as the problem of inducing a description (of some kind, even if only morpheme segmentation) of how orthographic words are built up given only raw text data of a language. We briefly go through the history and motivation of this problem. Next, over 200 items of work are listed with a brief characterization, and the most important ideas in the field are critically discussed. We summarize the achievements so far and give pointers for future developments.},
Author = {Hammarstrom, Harald and Borin, Lars},
ISSN = {08912017},
Journal = {Computational Linguistics},
Title = {Unsupervised Learning of Morphology},
Pages = {309 - 350},
Volume = {37},
Number = {2},
Year = {2011}
}

